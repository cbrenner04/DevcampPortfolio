<div class="inner cover">
  <h1 class="cover-heading">Tweets</h1>
  <div class="row">
    <% @tweets.each do |tweet| %>
      <div class="col-lg-6">
        <div class="tweets">
          <div class="card">
            <%= twitter_parser tweet %>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>
